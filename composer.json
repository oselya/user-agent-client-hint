{
    "name": "oselya/user-agent-client-hint",
    "description": "Lib to works with HTTP Client hints",
    "type": "library",
    "license": "MIT",
    "autoload": {
        "psr-4": {
            "Oselya\\UserAgentClientHint\\": "src/"
        },
        "files": [
            "src/constants.php",
            "src/functions.php"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "Oselya\\UserAgentClientHint\\Tests\\": "tests/"
        }
    },
    "config": {
        "preferred-install": "dist",
        "sort-packages": true,
        "optimize-autoloader": true
    },
    "require": {
        "php": ">=8.1"
    },
    "scripts": {
        "test": "vendor/bin/phpunit"
    },
    "require-dev": {
        "phpunit/phpunit": "^9.5"
    }
}
